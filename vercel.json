{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://financial-planner-pro.onrender.com/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://financial-planner-pro.onrender.com"
  }
} 